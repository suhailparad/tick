{
  "version": 2,
  "builds": [
    {
      "src": "tick-front/dist/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "tick-server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "tick-server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "tick-front/dist/$1"
    }
  ]
}